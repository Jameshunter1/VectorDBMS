build/
src/build/
_build/

# IDE and editor files
.vs/
.vscode/
.idea/
*.swp
*.swo
*~

# Git
.git/
.gitignore

# Documentation (already in repo, no need to copy twice)
*.md

# Test outputs
Testing/
*.log

# Python cache
__pycache__/
*.pyc
*.pyo

